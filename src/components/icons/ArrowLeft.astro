---
interface Props {
  size: string;
  className?: string;
}
const { size, className } = Astro.props;
---

<svg
  class={className}
  xmlns="http://www.w3.org/2000/svg"
  width={size}
  height={size}
  viewBox="0 0 24 24"
  fill="none"
  stroke="currentColor"
  stroke-width="2"
  stroke-linecap="round"
  stroke-linejoin="round"
  class="lucide lucide-chevron-left"><path d="m15 18-6-6 6-6"></path></svg
>
