---
interface Props {
  size: string;
  className?: string;
}
const { size, className } = Astro.props;
---

<svg
  class={className}
  xmlns="http://www.w3.org/2000/svg"
  width={size}
  height={size}
  viewBox="0 0 24 24"
  fill="none"
  stroke="currentColor"
  stroke-width="2"
  stroke-linecap="round"
  stroke-linejoin="round"
  class="lucide lucide-arrow-right"
  ><path d="M5 12h14"></path><path d="m12 5 7 7-7 7"></path></svg
>
